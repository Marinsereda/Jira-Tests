<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="HelloMaven2" verbose="1" group-by-instances="true">
    <test name="JiraTests" group-by-instances="true">
        <groups>
            <run>
                <exclude name="disabled"/>
                <include name="login"></include>
                <include name="AdminMenu"></include>
                <exclude name="IssuePage"></exclude>
            </run>
        </groups>
        <packages>
            <package name="com.testAuto"></package>
        </packages>


    </test>
</suite>
